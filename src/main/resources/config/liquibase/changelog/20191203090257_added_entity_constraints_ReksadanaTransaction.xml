<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ReksadanaTransaction.
    -->
    <changeSet id="20191203090257-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="buy_order_id"
                                 baseTableName="reksadana_transaction"
                                 constraintName="fk_reksadana_transaction_buy_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reksadana_switching"/>

        <addForeignKeyConstraint baseColumnNames="sell_order_id"
                                 baseTableName="reksadana_transaction"
                                 constraintName="fk_reksadana_transaction_sell_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reksadana_switching"/>

        <addForeignKeyConstraint baseColumnNames="reksadana_id"
                                 baseTableName="reksadana_transaction"
                                 constraintName="fk_reksadana_transaction_reksadana_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reksadana"/>

        <addForeignKeyConstraint baseColumnNames="switching_id"
                                 baseTableName="reksadana_transaction"
                                 constraintName="fk_reksadana_transaction_switching_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reksadana_switching"/>

    </changeSet>
</databaseChangeLog>
