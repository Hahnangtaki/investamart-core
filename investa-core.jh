entity AccountReksadana {
	costPrice BigDecimal, //(18,4)
    qtyOnHand BigDecimal, //(18,4)
    qtyDeposit BigDecimal, //(18,4)
    qtyWithdraw BigDecimal, //(18,4)
    profitLoss BigDecimal //(18,4)
}

relationship OneToMany {
	JenisReksadana to Reksadana
}

relationship OneToMany {
	KategoriReksadana to Reksadana
}

relationship OneToMany {
	Reksadana to ReksadanaAssetAllocation
}

relationship OneToMany {
	Reksadana to ReksadanaTransaction
}

relationship OneToMany {
	Reksadana to ReksadanaAsset
}

relationship OneToMany {
	Reksadana{sourceRek} to ReksadanaSwitching{source}
}

relationship OneToMany {
	Reksadana{targetRek} to ReksadanaSwitching{target}
}

relationship OneToMany {
	ReksadanaSwitching{switchingOrder} to ReksadanaTransaction{switching}
}

relationship OneToOne {
	ReksadanaTransaction{buyOrder} to ReksadanaSwitching{buy}
}

relationship OneToOne {
	ReksadanaTransaction{sellOrder} to ReksadanaSwitching{sell}
}

relationship OneToMany {
	AccountMember to AccountAddress
}

relationship OneToMany {
	AccountMember to AccountAuthorize
}

relationship OneToOne {
	AccountInstitution to AccountMember
}

relationship OneToOne {
	AccountIndividu to AccountMember
}

relationship OneToMany {
	Reksadana to AccountReksadana
}

relationship OneToMany {
	AccountMember to AccountReksadana
}

relationship OneToMany {
	AccountMember to AccountBank
}

paginate ReksadanaSwitching with pagination
paginate ReksadanaTransaction with pagination

dto * with mapstruct

service all with serviceImpl
