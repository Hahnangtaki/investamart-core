entity Reksadana {
	//default
	kodeReksadana String maxlength(20),
    namaReksadana String maxlength(100),
    isinCode String maxlength(20),
    tanggalPeluncuran LocalDate,
    aum BigDecimal, //(18,4)
    unit BigDecimal, //(18,4)
    lastPosDate LocalDate,
    biayaPembelian BigDecimal, //(18,4)
    biayaPenjualan BigDecimal, //(18,4)
    biayaSwitching BigDecimal, //(18,4)
    notes String,
    status String maxlength(1),
    
    //Tambahan
    nav BigDecimal, //(18,4)
    
    //desain
    barometer Double,
    
    currencyId Long,
    issuerId Long,
    custodyId Long,
    //jenisReksadanaId Long,
    //kategoriReksadanaId Long,
    
