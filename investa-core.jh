    accountId Long,
    //reksadanaId Long,
    //targetReksadanaId Long,
    currencyId Long,
    //buyOrderId Long,
    //sellOrderId Long,
    
    createSystemDate LocalDate,
    createDate ZonedDateTime,
    createUserId Long,
    lastModificationSystemDate LocalDate,
    lastModificationDate ZonedDateTime,
    lastModificationUserId Long
}

entity ReksadanaTransaction {
	orderNo String maxlength(10),
