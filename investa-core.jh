entity KategoriReksadana {
	kodeKategori String maxlength(10),
    namaKategori String maxlength(50),
    
    createSystemDate LocalDate,
    createDate ZonedDateTime,
    createUserId Long,
    lastModificationSystemDate LocalDate,
    lastModificationDate ZonedDateTime,
    lastModificationUserId Long
}

entity JenisReksadana {
	kodeJenis String maxlength(10),
    namaJenis String maxlength(50),
    
    createSystemDate LocalDate,
    createDate ZonedDateTime,
    createUserId Long,
    lastModificationSystemDate LocalDate,
    lastModificationDate ZonedDateTime,
    lastModificationUserId Long
}

entity ReksadanaAsset {
	valueDate LocalDate,
    aum BigDecimal, //(18,4)
    unit BigDecimal, //(18,4)
    
    //Tambahan
    nav BigDecimal, //(18,4)
    
    //reksadanaId Long,
    
    createSystemDate LocalDate,
    createDate ZonedDateTime,
    createUserId Long,
    lastModificationSystemDate LocalDate,
    lastModificationDate ZonedDateTime,
    lastModificationUserId Long
